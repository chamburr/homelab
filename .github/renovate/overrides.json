{
  "packageRules": [],
  "regexManagers": [
    {
      "description": "Traefik",
      "fileMatch": ["kubernetes/crds/traefik/.+\\.yaml$"],
      "matchStrings": [
        "# renovate: registry=(?<registryUrl>\\S+) chart=(?<depName>\\S+)\n.*?v(?<currentValue>[^-\\s]*)\n"
      ],
      "datasourceTemplate": "helm"
    }
  ]
}
